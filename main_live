#FUNCTIONS


def isProfitable(trade_start_balance,sell_amount_list, gas_fee):
    if trade_start_balance < (float(max(sell_amount_list))-((float(gas_fee))*2)):
        logging.info("Profitable")
    else:
        logging.info("Losable")
    return


def swap_function(swap_functions_of_exchanges, exchange_name):
    if swap_functions_of_exchanges[exchange_name] == "AVAX":
        return "Use AVAX Function"
    elif swap_functions_of_exchanges[exchange_name] == "ETH":
        return "Use ETH Function"
    else:
        return logging.info("Contract have different function. Check Exchanges roter contract")



