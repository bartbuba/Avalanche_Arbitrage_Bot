    png_balance_wei = png_contract.functions.balanceOf(wallet_addr).call()
    png_balance = w3.fromWei(png_balance_wei, 'ether')
    eth_balance_wei = eth_contract.functions.balanceOf(wallet_addr).call()
    eth_balance = w3.fromWei(eth_balance_wei, 'ether')
    usdt_balance_wei = usdt_contract.functions.balanceOf(wallet_addr).call()
    usdt_balance = w3.fromWei(usdt_balance_wei, 'ether')
    sherpa_balance_wei = sherpa_contract.functions.balanceOf(wallet_addr).call()
    sherpa_balance = w3.fromWei(sherpa_balance_wei, 'ether')
    logging.info("")
    logging.info("")
    logging.info("New Check is starting...")
    logging.info("---------- WALLET BALANCES ----------")
    logging.info("AVAX: " + str(avax_balance))
    logging.info("PNG: " + str(png_balance))
    logging.info("ETH: " + str(eth_balance))
    logging.info("USDT: " + str(usdt_balance))
    logging.info("SHERPA: " + str(sherpa_balance))
    logging.info(" ")

    #ENGINE
    #PAIR COMPARISION ACCROSS EXCHANGES


    #PAIRS BUY CALC.

    trade_amount = float(avax_balance) - 0.5
    trade_start_balance = trade_amount
    trade_amount_wei = w3.toWei(trade_amount, 'ether')


    for pair in pairsList:
        logging.info("")
        logging.info("NEW PAIR:" + pair.upper())
        exchanges_have_this_pair = pair_contracts_list[pair]

        buy_price_list = []
        sell_price_list =[]
        buy_amount_list = []
        sell_amount_list = []
        buy_exchange_list = []
        sell_exchange_list = []

        logging.info("---------- BUY PAIRS ----------")
        for pairsList in exchanges_have_this_pair:

            exchange_pair_contract = pair_contracts_list[pair][pairsList]
            router = exchanges_router_contracts[pairsList]

            token0 = exchange_pair_contract.functions.token0().call()
            token1 = exchange_pair_contract.functions.token1().call()        

            reserves_wei = exchange_pair_contract.functions.getReserves().call()
            reserveOfToken0_wei = reserves_wei[0]
            reserveOfToken1_wei = reserves_wei[1]

            #Decimal Arranger For USDT
            if tokenList[token1] == "USDT":
                reserveOfToken1_wei = reserves_wei[1]*1000000000000
            elif tokenList[token0] == "USDT":
                reserveOfToken0_wei = reserves_wei[0]*1000000000000
            else:            
                pass

            reserveOfToken0 = w3.fromWei(reserveOfToken0_wei, 'ether')
            reserveOfToken1 = w3.fromWei(reserveOfToken1_wei, 'ether')

            currency_rate = reserveOfToken0/reserveOfToken1
            

            logging.info("Exchange: " + pairsList.upper())
            logging.info(tokenList[token0] + "inPool:" + str(round(reserveOfToken0, 0)))
            logging.info(tokenList[token1] + "inPool:" + str(round(reserveOfToken1, 0)))
            logging.info(tokenList[token0]+"/"+tokenList[token1]+" Currency Rate: " + str(round(currency_rate, 4)),)


            #Finding Avax in Pair Pool For Buy
            if tokenList[token0] == "AVAX":
                reserveIn = int(reserveOfToken0_wei)
                reserveOut = int(reserveOfToken1_wei)
                pair_pool_name = tokenList[token0] + "/" + tokenList[token1]
            else:
                reserveIn = int(reserveOfToken1_wei)
                reserveOut = int(reserveOfToken0_wei)
                pair_pool_name = tokenList[token1] + "/" + tokenList[token0]

            #Predicted Amount to Buy Token
            amountIn = trade_amount_wei
            maxAmountOut_wei =  router.functions.getAmountOut(amountIn, reserveIn, reserveOut).call()
            maxAmountOut = w3.fromWei(maxAmountOut_wei, 'ether')
            logging.info(pair_pool_name + " : " + str(round(maxAmountOut, 4)))
            
            #Price Impact calculation
            #Starting Pool
            pI_before_trade = reserveIn/reserveOut
    
            #Traded Pool
            pI_after_trade = (reserveIn+trade_amount_wei)/(reserveOut-maxAmountOut_wei)
    
            price_impact = ((abs(((pI_after_trade / pI_before_trade) -1) * 100)) / 2)
            logging.info("Price Impact: " + str(round(price_impact, 2)) + "%" )
    
            slippage = 0.01*100
            
            if slippage > price_impact:
                buy_price_list.append(currency_rate)
                buy_amount_list.append(maxAmountOut)
                buy_exchange_list.append(pairsList)
                logging.info("Trade will occur")
            else:
                logging.info("pI is over 1 %")
                pass
            logging.info(buy_price_list)
            logging.info(buy_amount_list)
            logging.info(buy_exchange_list)

        #Best Exchange 
        cheap_buy_amount = max(buy_amount_list) 
        cheap_exchange = buy_exchange_list[buy_amount_list.index(cheap_buy_amount)]

        logging.info("")
        logging.info("Cheapest Exc: " + cheap_exchange)
        logging.info("Get Best Amount: " + str(cheap_buy_amount))


        #FUNCTION SEPERATION FOR ROUTERS AND GAS PRICE CALCULATION FOR BUY
        
        router = exchanges_router_contracts[cheap_exchange]

        slippage = 1
        currency_rate = buy_price_list[buy_amount_list.index(max(buy_amount_list))]

        #Parameters
        if tokenList[token0] == "AVAX":
            token_In = token0
            token_Out = token1
        else:
            token_In = token1
            token_Out = token0
        path = [token_In, token_Out]
        to = wallet_addr
        deadline = int(time.time()*1000 + 1000*60*10) #deadline is 10 minutes
        amountOutMin = int((trade_amount_wei - trade_amount_wei*slippage)*float(currency_rate) )
        params = (amountOutMin, path, to, deadline)

        swapFunction = str(swap_function(swap_functions_of_exchanges, cheap_exchange))
        if swapFunction == "Use AVAX Function":
            gas_of_contract = router.functions.swapExactAVAXForTokens(amountOutMin, path, to, deadline).estimateGas({
                'gas': 1000000, 
                'gasPrice': w3.eth.gas_price, 
                'from': wallet_addr, 
                'nonce': w3.eth.get_transaction_count(wallet_addr),
                'value': trade_amount_wei
            })

        elif swapFunction == "Use ETH Function":
            gas_of_contract = router.functions.swapExactETHForTokens(amountOutMin, path, to, deadline).estimateGas({
                'gas': 1000000, 
                'gasPrice': w3.eth.gas_price, 
                'from': wallet_addr, 
                'nonce': w3.eth.get_transaction_count(wallet_addr),
                'value': trade_amount_wei
            })

        else:
            logging.info("Error: Check Contract")

        #Gas Fee of Buy
        gas_fee_wei = (w3.eth.gas_price)*gas_of_contract
        gas_fee = w3.fromWei(gas_fee_wei, 'ether')

        logging.info("Gas Fee: " + str(gas_fee) + " AVAX")
        logging.info("")





        #SELL 
        logging.info("---------- SELL PAIRS ----------")
        for exchange_name in exchanges_have_this_pair:

            exchange_pair_contract = pair_contracts_list[pair][exchange_name]
            router = exchanges_router_contracts[exchange_name]

            token0 = exchange_pair_contract.functions.token0().call()
            token1 = exchange_pair_contract.functions.token1().call()

            reserves_wei = exchange_pair_contract.functions.getReserves().call()
            reserveOfToken0_wei = reserves_wei[0]
            reserveOfToken1_wei = reserves_wei[1]

            #Decimal Arranger For USDT
            if tokenList[token1] == "USDT":
                reserveOfToken1_wei = reserves_wei[1]*1000000000000
            elif tokenList[token0] == "USDT":
                reserveOfToken0_wei = reserves_wei[0]*1000000000000
            else:
                pass

            reserveOfToken0 = w3.fromWei(reserveOfToken0_wei, 'ether')
            reserveOfToken1 = w3.fromWei(reserveOfToken1_wei, 'ether')

            currency_rate = reserveOfToken0/reserveOfToken1


            logging.info("Exchange: " + exchange_name.upper())
            logging.info(tokenList[token0] + "inPool:" + str(round(reserveOfToken0, 0)))
            logging.info(tokenList[token1] + "inPool:" + str(round(reserveOfToken1, 0)))
            logging.info(tokenList[token0]+"/"+tokenList[token1]+" Currency Rate: " + str(round(currency_rate, 4)))


            #Finding Avax in Pair Pool For Sell
            if tokenList[token0] == "AVAX":
                reserveOut = int(reserveOfToken0_wei)
                reserveIn = int(reserveOfToken1_wei)
                pair_pool_name = tokenList[token0] + "/" + tokenList[token1]
            else:
                reserveOut = int(reserveOfToken1_wei)
                reserveIn = int(reserveOfToken0_wei)
                pair_pool_name = tokenList[token1] + "/" + tokenList[token0]


            #Predicted Amount to Sell Token
            amountIn = w3.toWei(cheap_buy_amount, 'ether')
            maxAmountOut_wei =  router.functions.getAmountOut(amountIn, reserveIn, reserveOut).call()
            maxAmountOut = w3.fromWei(maxAmountOut_wei, 'ether')

            logging.info(pair_pool_name + " : " + str(round(maxAmountOut, 4)))
            
            #Price Impact calculation
            #Starting Pool
            pI_before_trade = reserveIn/reserveOut
    
            #Traded Pool
            pI_after_trade = (reserveIn + amountIn) / (reserveOut - maxAmountOut_wei)
    
            price_impact = ((abs(((pI_after_trade / pI_before_trade) -1) * 100)) / 2)
            logging.info("Price Impact: " + str(round(price_impact, 2)) + "%" )
    
            slippage = 0.01*100
            
            if slippage > price_impact:
                sell_price_list.append(currency_rate)
                sell_amount_list.append(maxAmountOut)
                sell_exchange_list.append(exchange_name)
                logging.info("Trade will occur")
            else:
                logging.info("pI is over 1%")
                pass
            logging.info(sell_price_list)
            logging.info(sell_amount_list)
            logging.info(sell_exchange_list)

        expensive_sell_amount = max(sell_amount_list) 
        expensive_exchange = sell_exchange_list[sell_amount_list.index(max(sell_amount_list))]

        logging.info("")
        logging.info("Expensive Exc: " + expensive_exchange)
        logging.info("Expensive Sell Amount: " + str(expensive_sell_amount))
        logging.info("")


        #SUMMARY OF POSSIBLE TRADES For Each Pair

        if tokenList[token0] == "AVAX":
            token_symbol = tokenList[token1]
        else:
            token_symbol = tokenList[token0]

        possible_net_profit = (((float(expensive_sell_amount))-((float(gas_fee))*2))/trade_start_balance) - 1

        logging.info("")
        logging.info("---------- SUMMARY ----------")
        logging.info("Trade Start : " + str(round(trade_start_balance,3)) + " AVAX",)
        logging.info("Date and Time: " + str(datetime.datetime.now()))
        logging.info("Cheapest Exchange: " + cheap_exchange)
        logging.info("Expensive Exchange: " + expensive_exchange)
        logging.info("Path: " + "AVAX => " + token_symbol + " => AVAX")
        logging.info("Math: " + str(round(trade_start_balance,3)) + " => " + str(round(cheap_buy_amount,3)) +  " => " + str(round(expensive_sell_amount,3)))
        logging.info("Predicted Total Gas Fee: " + str((float(gas_fee))*2) + "AVAX")
        logging.info("Possible Profit: " + str(round((possible_net_profit*100), 4)) + "%")



        #AFTER FINDING PROFITABLE TRADE - MAIL + BUY + SELL
        if possible_net_profit > 0:

            logging.info("Found Possible Net Profit")
            logging.info("Attempting to send mail...")

            smtp_server = "smtp.gmail.com"
            port = 587  # For starttls
            sender_email = "avax.arbitrage@gmail.com"
            password = "c2u74SALDt3"

            # Create a secure SSL context
            context = ssl.create_default_context()

            # Try to log in to server and send email
            try:
                server = smtplib.SMTP(smtp_server,port)
                server.ehlo() # Can be omitted
                server.starttls(context=context) # Secure the connection
                server.ehlo() # Can be omitted
                server.login(sender_email, password)

                subject = "Trial Mail"
                sender_email = "avax.arbitrage@gmail.com"
                receiver_email = "avax.arbitrage@gmail.com"
                message = """\
                Subject: Trade Found

                Robot found profitable trade and attended to trade!
                {sell_exchange: }
                {buy_exchange: }
                Check NOW!

                """
                '''message.format((sell_exchange = expensive_exchange), (buy_exchange = cheap_exchange ))
                '''
                server.sendmail(sender_email, receiver_email, message)
                logging.info("Mail has been sent")
            except Exception as e:
                # Print any error messages to stdout
                logging.info("Mail couldn't sent")
                logging.info(e)
            finally:
                server.quit()



            #AVAX => TOKEN SWAP - BUY
            logging.info("Attempting to calculate buy trade...")

            #Trade Math
            trade_amount_wei = w3.toWei(trade_start_balance, 'ether')
            slippage = 0.01
            
            #Currency arrangement for amountOutMin
            if tokenList[token0] == "AVAX":
                currency_rate = float((1/currency_rate))
            else:
                currency_rate = float(currency_rate)
            
            amountOutMin = int((trade_amount_wei - trade_amount_wei*slippage)*currency_rate )


            #Parameters
            router = exchanges_router_contracts[cheap_exchange]
            deadline = int(time.time()*1000 + 1000*60*10) #deadline is 10 minutes
            params = (amountOutMin, path, to, deadline)

            logging.info("Calculate buy trade is completed")

            #Seperate and Create Transaction
            logging.info("Attempting to seperate Smart Contract Buy Function and Create Transaction...")

            swapFunction = str(swap_function(swap_functions_of_exchanges, cheap_exchange))
            if swapFunction == "Use AVAX Function":
                buy_transaction = router.functions.swapExactAVAXForTokens(amountOutMin, path, to, deadline).buildTransaction({
                    'gas': 1000000, 
                    'gasPrice': w3.eth.gas_price, 
                    'from': wallet_addr, 
                    'nonce': w3.eth.get_transaction_count(wallet_addr),
                    'value': trade_amount_wei
                })

            elif swapFunction == "Use ETH Function":
                buy_transaction = router.functions.swapExactETHForTokens(amountOutMin, path, to, deadline).buildTransaction({
                    'gas': 1000000, 
                    'gasPrice': w3.eth.gas_price, 
                    'from': wallet_addr, 
                    'nonce': w3.eth.get_transaction_count(wallet_addr),
                    'value': trade_amount_wei
                })

            else:
                logging.info("Error on BUY SWAP: Check Contract")

            logging.info("Smart Contract Functions Seperated For Buy")
            logging.info("Attempting to Sign transaction...")


            #Sign transaction
            signed_txn = w3.eth.account.signTransaction(
                buy_transaction, 
                private_key = private_key
            )

            logging.info("Buy Transaction is Signed")
            logging.info("Attempting to Send Buy Transaction...")
            
            print(trade_amount_wei)
            print(w3.eth.gas_price)
            print(router.address)
            print(pair)
            print(cheap_exchange)
            print(expensive_exchange)
            
            #Send Buy Transaction
            buy_hex_output = w3.eth.sendRawTransaction(signed_txn.rawTransaction)
            logging.info("Buy Transaction has sent")

            logging.info("Buy Transaction is waiting to be mined...")
            
            #Stay until Transaction Completed
            tx_receipt = w3.eth.wait_for_transaction_receipt(buy_hex_output)
            logging.info(buy_hex_output)
            
            #Bug çıkabilir.Wallet balanceları ile de kontrol et!
            logging.info("Buy Transaction is mined")



            #TOKEN => AVAX SWAP - SELL
            logging.info("Attempting to calculate sell trade...")

            #Kaç token alındı?
            logging.info("Attempting to get token balance...")
            token_contract = tokens_contract_list[tokenList[token_Out]]
            token_balance_wei = token_contract.functions.balanceOf(wallet_addr).call()

            #Trade Math
            trade_amount_wei = token_balance_wei
            slippage = 0.01
            currency_rate = float(1/currency_rate)
            amountOutMin = int((trade_amount_wei - trade_amount_wei*slippage)*currency_rate )

            #Parameters
            router = exchanges_router_contracts[expensive_exchange]
            amountIn = trade_amount_wei
            path = [token_Out, token_In]
            to = wallet_addr
            deadline = int(time.time()*1000 + 1000*60*10) #deadline is 10 minutes
            params = (amountIn, amountOutMin, path, to, deadline)

            logging.info("Calculate sell trade is completed")

            #Seperate and Create Transaction

            logging.info("Attempting to seperate Smart Contract Sell Function and Create Transaction...")

            swapFunction = str(swap_function(swap_functions_of_exchanges, expensive_exchange))
            if swapFunction == "Use AVAX Function":
                sell_transaction = router.functions.swapExactTokensForAVAX(amountIn, amountOutMin, path, to, deadline).buildTransaction({
                    'gas': 1000000, 
                    'gasPrice': w3.eth.gas_price, 
                    'from': wallet_addr, 
                    'nonce': w3.eth.get_transaction_count(wallet_addr)
                })

            elif swapFunction == "Use ETH Function":
                sell_transaction = router.functions.swapExactTokensForETH(amountIn, amountOutMin, path, to, deadline).buildTransaction({
                    'gas': 1000000, 
                    'gasPrice': w3.eth.gas_price, 
                    'from': wallet_addr, 
                    'nonce': w3.eth.get_transaction_count(wallet_addr)
                })

            else:
                logging.info("Error on SELL SWAP: Check Contract")

            logging.info("Smart Contract Functions Seperated For Sell")
            logging.info("Attempting to Sign Sell transaction...")


            #Sign transaction
            signed_txn = w3.eth.account.signTransaction(
                sell_transaction, 
                private_key = private_key
            )

            logging.info("Sell Transaction is Signed")
            logging.info("Attempting to Send Sell Transaction...")

            #Send Sell Transaction
            sell_hex_output = w3.eth.sendRawTransaction(signed_txn.rawTransaction)
            logging.info("Sell Transaction has sent")

            logging.info("Sell Transaction is waiting to be mined...")

            #Stay until Transaction Completed
            tx_receipt = w3.eth.wait_for_transaction_receipt(sell_hex_output)
            logging.info(sell_hex_output)

            #Bug çıkabilir.Wallet balanceları ile de kontrol et!
            logging.info("Sell Transaction is mined")

        else:
            pass



    print("Check is completed at " + str(datetime.datetime.now()))
